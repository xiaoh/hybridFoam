FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      sampleDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

interpolationScheme cellPoint;

setFormat       gnuplot;

sets
(
    leftPatch
    {
        type    midPoint;
        axis    y;
        start   ( 2 0 1 );
        end     ( 2 2.0 1 );
        // nPoints 100;
    }
);

surfaces        ();

fields          (U);


